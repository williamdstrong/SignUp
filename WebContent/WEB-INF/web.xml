<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>SignUp</servlet-name>
    <servlet-class>webapp.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignUp</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ThankYou</servlet-name>
    <servlet-class>webapp.ThankYou</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ThankYou</servlet-name>
    <url-pattern>/ThankYou/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShowUsers</servlet-name>
    <servlet-class>webapp.ShowUsers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowUsers</servlet-name>
    <url-pattern>/ShowUsers/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditUser</servlet-name>
    <servlet-class>webapp.EditUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditUser</servlet-name>
    <url-pattern>/EditUser/*</url-pattern>
  </servlet-mapping>
</web-app>